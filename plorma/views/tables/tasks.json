{
    "overview": {
        "columns": [
            {"name": "id", "label": "Id"},
            {"name": "name", "label": "Title"},
            {"name": "weight", "label": "Priority", "width": "50"},
            {"name": "task_state", "label": "State", "width": "120"},
            {"name": "resolution", "label": "Resolution", "width": "120", "expand": true},
            {"name": "tags", "label": "Tags"}
        ],
        "settings": {
            "default-sort-field": "weight",
            "default-sort-order": "desc"
        },
        "filters": [
            {"field": "task_state",
             "expr": "^(?:(?!Closed).)*$",
             "regex": true,
             "label": "Hide closed",
             "active": true}
        ]
    },
    "sprintlog": {
        "columns": [
            {"name": "id", "label": "Id"},
            {"name": "name", "label": "Title"},
            {"name": "tags", "label": "Tags", "width": "100"},
            {"name": "weight", "label": "Priority", "width": "70"},
            {"name": "estimate", "label": "Estimate", "width": "70"}
        ],
        "settings": {
            "default-sort-field": "weight",
            "default-sort-order": "desc",
            "default-search": [["!= Closed", "task_state", false]]
        }
    }
}
